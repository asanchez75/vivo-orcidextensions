

<i>Note: profile mgmt display, shows all attributes + editing links</i>

<!-- biographical info about this contributor-->

<p>
<%= link_to "Edit my profile", edit_profile_path(@profile) %>
</p>

<table class="profileTable">
<tbody>
  <tr>
    <td colspan="2" class="profileName"><%= @profile.lastname %>, <%= @profile.firstname %> <%= @profile.middleinitials %></td>
  </tr>
  <tr>
    <td class="profileFieldLabel">ORCID:</td>
    <td class="profileDataCells"><%= @profile.cid %></td>
  </tr>
  <tr>
    <td class="profileFieldLabel">URL:</td>
    <td class="profileDataCells">http://orcid.org/cid/<%= @profile.cid %></td>
  </tr>
  <tr>
    <td class="profileFieldLabel">Other names:</td>
    <td class="profileDataCells">
<%=  
other_names = @profile.other_names
foo = other_names.map! {|n| n.name }
other_names.join("; ")  %>
    </td>
  </tr>
</table>



